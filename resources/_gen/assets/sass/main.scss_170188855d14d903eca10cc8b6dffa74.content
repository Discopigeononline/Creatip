body{font-family:times new roman,serif;color:#5776a4;background-color:#f7f3d7;line-height:1.5;margin:0}#page{height:100%;min-height:100vh;display:flex;flex-direction:column}#content-wrap{flex-grow:1 0 auto;margin:0 5vw;text-align:justify}#bg-hero{top:0;height:100vh;background-image:url(/images/hero-mobile.jpg);background-image:image-set("/images/hero-mobile.webp" type("image/webp"),"/images/hero-mobile.jpg" type("image/jpg"));background-attachment:fixed;background-position:50%;background-repeat:no-repeat;background-size:cover;display:flex;justify-content:center;align-items:center}.main-header{position:sticky;height:12vh;top:0;left:0;right:0;background-color:#5776a4}.navbar{display:flex;justify-content:flex-start;justify-content:space-between;align-items:center;height:12vh;margin:auto 3vw}.logo{height:150px;width:150px;user-select:none}#menu-wide{display:none}#overlay-button{justify-content:flex-end;cursor:pointer;z-index:4;user-select:none}#overlay-button span{height:6px;width:50px;border-radius:2px;background-color:#d9b567;position:relative;display:block;transition:all .25s ease-in-out}#overlay-button span:before{top:-10px}#overlay-button span:after{top:10px}#overlay-button span:before,#overlay-button span:after{height:6px;width:50px;border-radius:2px;background-color:#d9b567;position:absolute;content:"";transition:all .2s ease-in-out}input[type=checkbox]{display:none}input[type=checkbox]:checked~#overlay{display:block}input[type=checkbox]:checked~#overlay-button:hover span,input[type=checkbox]:checked~#overlay-button span{background:0 0}input[type=checkbox]:checked~#overlay-button span:before{transform:rotate(45deg)translate(7px,7px);opacity:1}input[type=checkbox]:checked~#overlay-button span:after{transform:rotate(-45deg)translate(7px,-7px)}#overlay{position:fixed;top:0;left:0;width:100%;height:100%;background:#5776a4;z-index:3;display:none;position:fixed}#overlay #menu-mobile{display:flex;justify-content:center;align-items:center;flex-direction:column;text-align:center;height:100vh;z-index:4;list-style-type:none}#overlay #menu-mobile .menu-item{padding:1em}#overlay #menu-mobile .menu-item a{color:#d9b567;text-decoration:none;font-size:70px}.back_to_top{align-self:flex-end;margin:3vw 3vh;width:100px;height:100px}footer{display:flex;flex-direction:column;width:100%;background:#5776a4;font-size:.7em;text-align:center;text-decoration:none;background-color:#5776a4;color:#f7f3d7;font-size:20px}footer #footer-eco,footer #conception{display:inline-block;border-top:1px solid #f7f3d7;padding:9px}footer .creator{color:inherit;text-decoration:none}@media screen and (min-width:1000px){#overlay-button{display:none}#menu-wide{display:block;list-style:none;display:flex;justify-content:space-evenly;overflow:hidden;line-height:1.5em}#menu-wide .menu-wide-item{display:flex;justify-content:center;align-items:center;padding-right:2rem;font-size:1.6em;font-weight:bolder}#menu-wide .menu-wide-item a{color:#f7f3d7;text-decoration:none;transition:all .2s ease-in-out}#menu-wide .menu-wide-item a:hover{color:#d9b567}#bg-hero{background-image:url(/images/hero-desktop.jpg);background-image:image-set("/images/hero-desktop.webp" type("image/webp"),"/images/hero-desktop.jpg" type("image/jpeg"));background-attachment:fixed;background-size:100% 100%}}