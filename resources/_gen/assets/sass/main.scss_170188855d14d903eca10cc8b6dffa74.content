body{width:100vw;font-family:times new roman,serif;color:#2f4f4f;background-color:#f2e8cf;line-height:1.5;margin:0}#page{min-height:100vh;display:flex;flex-direction:column}#content-wrap{flex-grow:1}.main-header{position:sticky;height:5.5rem;top:0;left:0;right:0}nav ul{list-style:none}nav ul a{color:#2f4f4f;font-size:1.2rem;font-weight:700;padding:1rem 2rem;display:inline-block;transition:color .3s ease;text-decoration:none}nav ul a:hover{color:#8b4513}nav ul li{display:inline}footer{display:flex;flex-wrap:wrap;width:100%;background:tan;font-size:.7em;text-align:center;text-decoration:none}footer .back_to_top{border-style:solid;border-radius:100%}